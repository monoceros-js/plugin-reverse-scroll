{
  "name": "@monoceros/plugin-reverse-scroll",
  "version": "1.0.0",
  "description": "Reverse scroll plugin for @monoceros",
  "main": "dist/plugin-reverse-scroll.js",
  "scripts": {
    "pre-commit:msg": "echo 'Pre-commit checks...' && exit 0",
    "test": "npm run lint",
    "lint": "npm run lint:md",
    "lint:md": "remark .",
    "build": "npm run build:production && npm run build:md",
    "build:md": "remark readme.md --output",
    "build:dev": "npm run build:module && npm run build:browser_dev",
    "build:production": "npm run build:module && npm run build:browser_prod",
    "build:module": "rollup -c --environment BUILD:module",
    "build:browser_prod": "rollup -c --environment BUILD:browser_production",
    "build:browser_dev": "rollup -c --environment BUILD:browser_development"
  },
  "keywords": [
    "scroll",
    "javascript",
    "animation",
    "browser",
    "monoceros",
    "reverse",
    "scroll",
    "reverse scroll"
  ],
  "author": "FollyWolly <hello@folkertjan.nl> (https://folkertjan.nl)",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git://github.com/monoceros-js/plugin-reverse-scroll.git"
  },
  "pre-commit": {
    "run": [
      "test"
    ]
  },
  "devDependencies": {
    "@babel/core": "^7.9.0",
    "@babel/preset-env": "^7.9.0",
    "@rollup/plugin-commonjs": "^11.0.2",
    "@rollup/plugin-json": "^4.0.2",
    "@rollup/plugin-node-resolve": "^7.1.1",
    "core-js": "^3.6.4",
    "pre-commit": "^1.2.2",
    "regenerator-runtime": "^0.13.5",
    "remark-cli": "^8.0.0",
    "remark-toc": "^7.0.0",
    "rollup": "^2.3.2",
    "rollup-plugin-babel": "^4.4.0",
    "rollup-plugin-terser": "^5.3.0"
  }
}
